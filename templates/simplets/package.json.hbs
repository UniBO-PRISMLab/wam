{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "main": "thing.js",
  "scripts": {
    "build" : "wam build --typescript ./src/thing.ts ./dist/bundle.js",
    "debug" : "wot-runtime -debug ./dist/bundle.js"
  },
  "keywords": [
    {{#each keywords}}
      "{{this}}"{{#unless @last}},{{/unless}}
    {{/each}}
  ],
  "author": "{{author}}",
  "license": "{{license}}",
  "devDependencies": {
    "@node-wot/cli": "^0.6.2",
    "wam": "{{selfVersion}}"
  },
  "dependencies": {
    "@node-wot/core": "^0.6.2"
  }
}
