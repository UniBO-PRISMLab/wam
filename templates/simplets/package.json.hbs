{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "main": "thing.js",
  "scripts": {
    "build" : "wam build --typescript ./src/thing.ts ./dist/bundle.js",
    "debug" : "wot-servient --inspect-brk -f servient.config.json ./dist/bundle.js",
    "start:prod" : "wot-servient -f servient.config.json ./dist/bundle.js",
    "start": "npm run build && npm run start"
  },
  "keywords": [
    {{#each keywords}}
      "{{this}}"{{#unless @last}},{{/unless}}
    {{/each}}
  ],
  "author": "{{author}}",
  "license": "{{license}}",
  "devDependencies": {
    "@node-wot/cli": "^0.7.0",
    "@arces-wot/wam": "{{selfVersion}}",
    "@rollup/plugin-typescript": "^2.0.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@node-wot/core": "^0.7.0"
  }
}
